<Window x:Class="SimplyRugby2._0.ChangeStaffPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimplyRugby2._0"
        mc:Ignorable="d"
        Title="ChangeStaffPassword" Height="450" Width="500">
    <Grid>
        <Label Content="UserName:" HorizontalAlignment="Left" Margin="39,39,0,0" VerticalAlignment="Top"/>
        <Label Content="New Password:" HorizontalAlignment="Left" Margin="39,129,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="CoachUsernametxtbox" HorizontalAlignment="Left" Margin="102,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="CoachPasstxtbox" HorizontalAlignment="Left" Margin="102,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="DashReturn1" Content="Back To DashBoard" HorizontalAlignment="Left" Margin="295,254,0,0" VerticalAlignment="Top" Click="DashReturn1_Click"/>
        <Button x:Name="Confirm2" Content="Confirm Changes" HorizontalAlignment="Left" Margin="295,296,0,0" VerticalAlignment="Top" Click="Confirm2_Click"/>
        <CheckBox x:Name="StaffChange" Content="Change A Coach Password" HorizontalAlignment="Left" Margin="295,39,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
