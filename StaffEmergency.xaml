<Window x:Class="SimplyRugby2._0.StaffEmergency"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimplyRugby2._0"
        mc:Ignorable="d"
        Title="StaffEmergency" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="searchtxtbox" HorizontalAlignment="Left" Margin="58,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="DashReturnBtn" Content="Back To DashBoard" HorizontalAlignment="Left" Margin="400,38,0,0" VerticalAlignment="Top" Click="DashReturnBtn_Click"/>
        <Button x:Name="searchbtn" Content="Search For Contacts" HorizontalAlignment="Left" Margin="218,41,0,0" VerticalAlignment="Top" Click="searchbtn_Click"/>
        <DataGrid x:Name="ContactsDataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="58,122,108,122"/>

    </Grid>
</Window>
