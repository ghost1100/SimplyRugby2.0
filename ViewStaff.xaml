<Window x:Class="SimplyRugby2._0.ViewStaff"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimplyRugby2._0"
        mc:Ignorable="d"
        Title="ViewStaff" Height="450" Width="800">
    <Grid>
        <Button x:Name="DashReturnbtn" Content="Back To DashBoard" HorizontalAlignment="Left" Margin="649,45,0,0" VerticalAlignment="Top" Click="DashReturnbtn_Click"/>
        <TextBox x:Name="Searchalltxt" HorizontalAlignment="Left" Margin="69,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Searchbtn" Content="Search" HorizontalAlignment="Left" Margin="231,44,0,0" VerticalAlignment="Top" Click="Searchbtn_Click"/>
        <DataGrid x:Name="CoachDataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="46,151,46,88"/>
        <Button x:Name="ViewAllbtn" Content="View all Staff Members" HorizontalAlignment="Left" Margin="326,45,0,0" VerticalAlignment="Top" Click="ViewAllbtn_Click"/>
        <Button x:Name="ViewAllbtn2" Content="View All Coaches" HorizontalAlignment="Left" Margin="509,45,0,0" VerticalAlignment="Top" Click="ViewAllbtn2_Click"/>

    </Grid>
</Window>
